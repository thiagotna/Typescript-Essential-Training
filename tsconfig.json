{
    "compilerOptions": {
        "allowJs": true,
        "checkJs": true,
        "target": "ESNext", //JS version to be outputed
        "ignoreDeprecations": "5.0", //Ignore deprecated JS version for a specific Typrescript version
        //"noEmit": true, Emmit output file
        "outDir": "build", //folder output name,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        
    },
    "include": ["src/**/*"]
}